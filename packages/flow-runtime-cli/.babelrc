{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": 4
        },
        "exclude": [
          "transform-regenerator"
        ]
      }
    ],
    "stage-0",
    "react"
  ],
  "env": {
    "development": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": 4
            },
            "exclude": [
              "transform-regenerator"
            ]
          }
        ],
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-decorators-legacy",
        ["flow-runtime", {
          "warn": true,
          "annotate": true
        }]
      ],
    }
  },
  "passPerPreset": true
}